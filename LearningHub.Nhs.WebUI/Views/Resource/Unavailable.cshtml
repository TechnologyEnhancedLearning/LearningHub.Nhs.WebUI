@{
    ViewData["Title"] = "Resource not available";
}

<div class="bg-white">
  <div class="nhsuk-width-container app-width-container">
    <div class="nhsuk-grid-row">
      <div class="nhsuk-grid-column-full nhsuk-u-padding-top-9 nhsuk-u-padding-bottom-7">
        <h1 class="nhsuk-heading-xl">Resource not available</h1>
        <p>This resource is not available for you to access. If you need help, please contact the <a href="@ViewBag.SupportFormUrl" target="_blank">support team</a>.</p>
        <p>@DateTimeOffset.Now.ToString("d MMMM yyyy HH:mm:ss")</p>
      </div>
    </div>
  </div>
</div>
